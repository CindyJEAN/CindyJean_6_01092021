{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "warn",
    "indent": [
      "warn",//error
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "warn",//error
      "windows"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-unused-vars": "warn",
    "semi": [
      "error",
      "always"
    ],
    "complexity": [
      "warn"
    ],
    "default-case": [
      "warn",
      {
        "commentPattern": "^no\\sdefault"
      }
    ],
    "no-constructor-return": [
      "warn"
    ],
    "no-else-return": [
      "warn",
      {
        "allowElseIf": true
      }
    ],
    "no-empty-function": [
      "warn"
    ],
    "no-extra-bind": [
      "warn"
    ],
    "no-implicit-coercion": [
      "warn",
      {
        "boolean": true,
        "number": true,
        "string": true
      }
    ],
    "generator-star-spacing": [
      "warn"
    ],
    "no-useless-constructor": [
      "warn"
    ],
    "no-useless-rename": [
      "warn"
    ],
    "prefer-const": [
      "warn",
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "strict": [
      "warn"
    ],
    "no-delete-var": [
      "off"
    ]
  }
}